		<InputResourceID />
		<PhysicalParameter>
			<Name>SAMPLE_TIME_UTC</Name>
			<ParameterKey>SAMPLE_TIME_UTC</ParameterKey>
			<Description>Sample UTC in the form DD MM YYYY hh mm ss where                      
          DD   = day of month (01-31)
          MM   = month of year (01-12)
          YYYY = Gregorian Year AD
          hh   = hour of day     (00:23)
          mm   = minute of hour  (00-59)
          ss   = second of minute (00-60).
          </Description>
			<Caveats />
			<Cadence>PT1S</Cadence>
			<Units>Seconds</Units>    
			<UnitsConversion />
			<Support>
			  <Temporal />
			</Support>
		</PhysicalParameter>
		<PhysicalParameter>
			<Name>MAGNETIC_FIELD_VECTOR</Name>
			<ParameterKey>MAGNETIC_FIELD_VECTOR</ParameterKey>
			<Description>Magnetic field vector in GSE Coordinates (Bx, By, Bz).</Description>
			<Caveats />
			<Cadence>PT1S</Cadence>
			<Units>nT</Units>    
			<UnitsConversion />
			<CoordinateSystem>
				<CoordinateRepresentation>Cartesian</CoordinateRepresentation>
				<CoordinateSystemName>GSE</CoordinateSystemName>
			</CoordinateSystem>	
			<Dimension>
				<Size>3</Size>
				<Description>Three component vector: x, y, z</Description>
			</Dimension>
			<Measured>
				<Field>
					<FieldQuantity>Magnetic</FieldQuantity>
				</Field>
			</Measured>
		</PhysicalParameter>
		<PhysicalParameter>
			<Name>SPACECRAFT_POSITION_VECTOR</Name>
